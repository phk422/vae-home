/* components/menu-item/index.wxss */
.menu-item {
  display: flex;
  align-items: center;
  height: 104rpx;
  background-color: #fff;
  padding: 0 26rpx;
}

.menu-item .icon {
  width: 50rpx;
  height: 50rpx;
  background-color: #00f;
  margin-right: 20rpx;
}

.menu-item .content {
  flex: 1;
}

.menu-item .end-icon {
  text-align: right;
  width: 200rpx;
  font-size: 28rpx;
  color: #909297;
}

.arrow-icon {
  text-align: right;
  background: url(data:image/png;base64,) no-repeat;
  width: 50rpx;
  height: 50rpx;
}
